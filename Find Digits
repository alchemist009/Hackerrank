#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main(){
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++){
        int n;
        cin >> n;
        int p=0, n1 =0;
        int tn = n;
        while(tn>0)
            {
               int n1 = tn%10;
               tn = tn/10;
               if(n1>0 && n%n1 == 0)
                   {
                   p++;
               }
        }
        cout<<p<<endl;
    }
    return 0;
}
